<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.0 -->
  <object class="GtkGrid" id="main-grid">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="column_spacing">15</property>
    <property name="row_spacing">15</property>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <placeholder/>
    </child>
    <child>
      <object class="GtkGrid" id="left-button-grid">
	<property name="visible">True</property>
	<property name="can_focus">False</property>
	<property name="column_spacing">10</property>
	<property name="halign">fill</property>
	<property name="column_homogeneous">true</property>
	<child>
	  <object class="GtkButton" id="clear-button">
	    <property name="label" translatable="yes">Clear</property>
	    <property name="visible">True</property>
	    <property name="can_focus">True</property>
	    <property name="receives_default">True</property>
	    <property name="use_action_appearance">False</property>
	    <property name="width_request">100</property>
	    <property name="halign">start</property>
	  </object>
	  <packing>
	    <property name="left_attach">0</property>
	    <property name="top_attach">0</property>
	    <property name="width">1</property>
	    <property name="height">1</property>
	  </packing>
	</child>
	<child>
	  <object class="GtkButton" id="apply-button">
	    <property name="visible">True</property>
	    <property name="can_focus">True</property>
	    <property name="receives_default">True</property>
	    <property name="use_action_appearance">False</property>
	    <property name="width_request">100</property>
	    <property name="halign">end</property>
	    <child>
	      <object class="GtkAlignment" id="apply-button-alignment">
		<property name="xalign">0.5</property>
		<property name="xscale">0</property>
		<child>
		  <object class="GtkGrid" id="apply-button-box">
		    <property name="orientation">horizontal</property>
		    <child>
		      <object class="GtkLabel" id="apply-button-label">
			<property name="label">LaTeX</property>
		      </object>
		    </child>
		    <child>
		      <object class="GtkImage" id="apply-button-image">
			<property name="icon_name">media-playback-start-symbolic</property>
		      </object>
		    </child>
		  </object>
		</child>
	      </object>
	    </child>
	  </object>
	  <packing>
	    <property name="left_attach">1</property>
	    <property name="top_attach">0</property>
	    <property name="width">1</property>
	    <property name="height">1</property>
	  </packing>
	</child>
      </object>
      <packing>
	<property name="left_attach">0</property>
	<property name="top_attach">1</property>
	<property name="width">1</property>
	<property name="height">1</property>
      </packing>
    </child>
    <child>
      <object class="GtkGrid" id="middle-button-grid">
	<child>
	  <object class="GtkToggleButton" id="history-button">
	    <property name="label" translatable="yes">History</property>
	    <property name="visible">True</property>
	    <property name="can_focus">True</property>
	    <property name="receives_default">True</property>
	    <property name="use_action_appearance">False</property>
	    <property name="width_request">100</property>
	  </object>
	  <packing>
	    <property name="left_attach">0</property>
	    <property name="top_attach">0</property>
	    <property name="width">1</property>
	    <property name="height">1</property>
	  </packing>
	</child>
      </object>
      <packing>
	<property name="left_attach">1</property>
	<property name="top_attach">1</property>
	<property name="width">1</property>
	<property name="height">1</property>
      </packing>
    </child>
  </object>
</interface>
